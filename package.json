{
	"name": "cc14-solo-api",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node ./src/index.js",
		"dev": "nodemon ./src/index.js",
		"lint": "eslint --fix --ext .js .",
		"test": "mocha test && yarn lint",
		"orm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
		"makeMigrations": "yarn orm migration:generate",
		"migrate": "yarn orm migration:run",
		"rollback": "echo 'FIXME!'",
		"seeder": "ts-node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js",
		"seed": "yarn seeder --configName src/ormconfig.ts  seed",
		"seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
		"seed:run": "ts-node  ./node_modules/typeorm-seeding/dist/cli.js seed",
		"pre-commit": "yarn lint"
	},
	"dependencies": {
		"@types/chai": "^4.2.12",
		"@types/chai-http": "^4.2.0",
		"@types/lodash.pick": "^4.4.6",
		"@types/mocha": "^8.0.3",
		"@types/pg": "^7.14.4",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"husky": "^4.2.5",
		"jsonwebtoken": "^8.5.1",
		"lodash.pick": "^4.4.0",
		"orm": "^5.0.5",
		"pg": "^8.3.3",
		"prettier": "^2.1.1",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^9.0.0",
		"typeorm": "^0.2.25",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.0.2",
		"underscore": "^1.11.0"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-problems": "^5.0.0",
		"eslint-plugin-prettier": "^3.1.4",
		"mocha": "^8.1.3",
		"nodemon": "^2.0.4",
		"pretty-quick": "^3.0.0"
	}
}
